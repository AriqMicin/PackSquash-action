{
    "problemMatcher": [
        {
            "owner": "packsquash-error",
            "severity": "error",
            "pattern": [
                {
                    "regexp": "^! (.+)$",
                    "message": 1
                }
            ]
        },
        {
            "owner": "packsquash-warning",
            "severity": "warning",
            "pattern": [
                {
                    "regexp": "^\\* (.+)$",
                    "message": 1
                }
            ]
        }
    ]
}
